{
  "manifest_version": 2,
  "name": "Greedy Snake",
  "version": "1.0",
  "author": "Molly & Yuhang",
  "description": "Press arrow keys and let the greedy snake eat as many HTML elements as possible!",

  "permissions":[
    "activeTab"
  ],
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js":["p5.js","sketch.js","findAndReplaceDomText.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon/snake-16.png"
    }
  },
  "icons": {
    "48": "icon/snake-32.png",
    "128": "icon/snake-128.png"
  }
}
